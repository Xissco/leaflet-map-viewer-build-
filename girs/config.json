{
    "navbar": {
      "url": "https://comunidadsostenible.ucacue.edu.ec/atlas/datos-historicos/",
      "label": "GIRS - Gestión Integral de Residuos Sólidos"
    },    
    "map1": {
      "geoserverUrl": "https://gis.comunidadsostenible.ucacue.edu.ec/geoserver/wms",
