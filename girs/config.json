        "variable": "orrsn",
        "type": "string",
        "style": "orrsn_style"
      },
      "reglamento_cobro": {
        "label": "Reglamento para el Cobro de la GIRS",
        "variable": "rcosn",
        "type": "string",
        "style": "rcosn_style"
      },
      "tipo_disposicion": {
        "label": "Tipo de Disposición Final Utilizado",
        "variable": "dftsi",
        "type": "string",
        "style": "dftsi_style"
      },
      "modo_cobranza": {
